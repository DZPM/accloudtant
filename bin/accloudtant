#!/usr/bin/env python
import click
from accloudtant.aws.prices import Prices
from accloudtant.aws.reports import Reports


@click.group()
def cli():
    pass


@cli.command()
def print():
    click.echo(Prices())


@cli.command()
def report():
    click.echo(Reports())

if __name__ == '__main__':
    cli()
